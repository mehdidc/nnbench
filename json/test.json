{
    "optim": {
        "nb_epoch": 1, 
        "algo": {
            "name": "SGD",
            "params": {
                "lr": 0.1,
                "momentum": 0.9
            }
        }, 
        "batch_size": 128, 
        "pred_batch_size": 1024, 
        "regularization":{
            "l2": 0.0001,
            "l1": 0
        },
        "lr_schedule": {
            "name": "cifar", 
            "params":{
            }
        }, 
        "early_stopping":{
            "name": "none",
            "params":{
            }
        },
        "checkpoint":{
            "loss": "val_accuracy",
            "save_best_only": true
        },
        "seed": 42, 
        "budget_secs": 86400
    }, 
    "model": {
        "name": "squeezenet",
        "params": {
            "squeeze_filters_size": [16, 16, 32,  32,  48,  48,  64, 64    ],
            "expand_filters_size":  [64, 64, 128, 128, 192, 192, 256, 256  ],
            "pool":                 [0,  0,  1,   0,    0,  1,   0,   0,  1],
            "dropout": 0.5
        }, 
        "loss": "categorical_crossentropy",
        "metrics": ["accuracy"]
    }, 
    "data": {
        "preprocessing":[
        ],
        "seed": 1, 
        "shuffle": true, 
        "valid_ratio": 0.1,
        "name": "mnist"
    }
}
