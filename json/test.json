{
    "optim": {
        "loss": "mean_squared_error",
        "metrics": ["mean_squared_error"],
        "nb_epoch": 1000, 
        "algo": {
            "name": "RMSprop",
            "params": {
                "lr": 0.0001
            }
        }, 
        "batch_size": 128, 
        "pred_batch_size": 1024, 
        "regularization":{
            "l2": 0.0001,
            "l1": 0
        },
        "lr_schedule":{
            "name": "decrease_when_stop_improving",
            "params": {
                "loss": "val_mean_squared_error",
                "shrink_factor": 2,
                "patience": 100,
                "min_lr": 0.00000001
            }
        },
        "early_stopping":{
            "name": "none",
            "params":{
            }
        },
        "checkpoint":{
            "loss": "val_mean_squared_error",
            "save_best_only": true
        },
        "seed": 42, 
        "budget_secs": 86400
    }, 
    "model": {
        "name": "resnet",
        "params": {
            "size_blocks":[2,  2],
            "nb_filters": [16, 32],
            "block": "reference",
            "option": "B"
        }
    }, 
    "data": {
        "preprocessing":[
        ],
        "seed": 1, 
        "shuffle": true, 
        "valid_ratio": 0.3,
        "name": "ilc"
    }
}
